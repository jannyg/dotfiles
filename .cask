# Install native apps
brew tap phinze/homebrew-cask
brew install brew-cask

function installcask() {
	brew cask install "${@}" 2> /dev/null
}

installcask google-chrome
installcask iterm2
installcask virtualbox
installcask vagrant
installcask vlc
installcask spotify
installcask lastpass-universal
installcask amphetamine
installcask spectacle
installcask rocket-chat
installcask slack
installcask keybase
installcask ngrok
